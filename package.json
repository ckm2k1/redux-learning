  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.3.0",
    "browserify": "13.0.0",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.1"
  }
}
