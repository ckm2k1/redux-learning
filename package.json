  "private": true,
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "setup": "rm -rf dist && mkdir dist",
    "build": "npm run setup && npm run build:js",
    "build:js": "browserify src/index.js -t [ babelify --presets [ es2015 ] ] -o dist/app.js",
